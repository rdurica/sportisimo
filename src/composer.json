{
  "name": "rdurica/sportisimo",
  "description": "Sportisimo web App",
  "keywords": [
    "nette"
  ],
  "type": "project",
  "license": [
    "MIT",
    "BSD-3-Clause",
    "GPL-2.0",
    "GPL-3.0"
  ],
  "require": {
    "php": ">= 8.2",
    "nette/application": "^3.1",
    "nette/bootstrap": "^3.1",
    "nette/caching": "^3.1",
    "nette/database": "^3.1",
    "nette/di": "^3.0",
    "nette/finder": "^2.5",
    "nette/forms": "^3.1",
    "nette/http": "^3.1",
    "nette/mail": "^3.1",
    "nette/robot-loader": "^3.3",
    "nette/security": "^3.1",
    "nette/utils": "^3.2",
    "latte/latte": "^2.11 || ^3.0",
    "tracy/tracy": "^2.8"
  },
  "require-dev": {
    "nette/tester": "^2.3",
    "squizlabs/php_codesniffer": "*"
  },
  "autoload": {
    "psr-4": {
      "App\\": "App"
    }
  },
  "minimum-stability": "stable",
  "config": {
  },
  "scripts": {
    "phpcs": "./vendor/bin/phpcs --standard=PSR12 App/",
    "phpcbf": "./vendor/bin/phpcbf --standard=PSR12 App/",
    "post-install-cmd": [
      "./vendor/bin/phpcs --config-set colors 1",
      "./vendor/bin/phpcs --config-set report_width 140"
    ]
  }
}